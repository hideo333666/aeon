<%= form_with(model: project, url: projects_path, local: true) do |form| %>
  <div>
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>
  <div>
    <%= form.label :description, "説明" %>
    <%= form.text_area :description %>
  </div>
  <%= form.submit "プロジェクトを作成", class: "btn btn-primary" %>
<% end %>
